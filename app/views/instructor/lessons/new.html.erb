<br />
<div class="booyah-box col-8 offset-2">
    <h1> Add a New Lesson</h1>
    <%= simple_form_for @lesson, url: instructor_section_lessons_path(current_section) do |f| %>
        <%= f.input :title %>
        <%= f.input :subtitle %>
        <%= f.input :video %>
        <br />
        <%= f.submit "Add Your Lesson", class: 'btn btn-primary' %>
        <br />
        <br />
    <% end %>
</div>